{
  "name": "One-Click Cache Cleaner",
  "description": "Read Bookmarks and save them offline",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "browsingData"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/speedometer-16.png",
      "32": "assets/icons/speedometer-32.png",
      "64": "assets/icons/speedometer-64.png",
      "128": "assets/icons/speedometer-128.png",
      "256": "assets/icons/speedometer-256.png"
    },
    "default_title": "One-Click Cache Cleaner"
  }
}