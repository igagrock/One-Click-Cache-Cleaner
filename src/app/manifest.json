{
  "name": "one-click Cache cleaner",
  "description": "Read Bookmarks and save them offline",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "browsingData"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/cpu1-16.png",
      "64": "assets/icons/cpu1-64.png",
      "128": "assets/icons/cpu1-128.png"
    },
    "default_title": "Silent Cache Cleaner"//,
    // "default_popup": "popup.html"
  }
}