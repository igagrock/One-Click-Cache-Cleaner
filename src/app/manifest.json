{
  "name": "One-Click Cache Cleaner",
  "description": "Silently clean cache with just one click",
  "manifest_version": 2,
  "version": "1.0.0.3",
  "permissions": [
    "browsingData",
    "notifications",
    "cookies",
    "history",    
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "One-Click Cache Cleaner"
  }
}